  "scripts": {
    "play": "npx --yes create-react-play@latest -p",
    "dev": "vite --mode development --host",
    "dev:force": "npx kill-port 3000 && clear && vite --mode development --host --force --config vite.config.ts",
    "start:nolint": "npx --yes create-react-play@latest -p && vite",
    "start": "npx --yes create-react-play@latest -p && npm run lint && vite",
    "build": "vite build --mode production",
    "postsnap": "vite build --mode production && react-snap",
    "prod": "tsc && vite --mode production --host",
    "snap": "vite build --mode production && react-snap",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx}",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,css,md}\" --config ./.prettierrc"
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "inlineCss": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
